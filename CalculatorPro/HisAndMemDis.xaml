<UserControl x:Class="CalculatorPro.HisAndMemDis"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CalculatorPro"
             mc:Ignorable="d" 
             MinHeight="501" MinWidth="248.6" MaxWidth="330" Background="#e6e6e6">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="49"/>
            <RowDefinition Height="419*"/>
            <RowDefinition Height="40.75"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0" LastChildFill="False">
            <Button DockPanel.Dock="Left" Style="{StaticResource btnHisAndMem}" Margin="10,10,10,5.8">历史记录</Button>
            <Button DockPanel.Dock="Left" Style="{StaticResource btnHisAndMem}" Width="34" Margin="20,10,10,5.8">内存</Button>
        </DockPanel>
        <DockPanel Grid.Row="2">
            <Button  DockPanel.Dock="Left" Width="40.75" Height="40.75" HorizontalAlignment="Right" Background="#e6e6e6" BorderThickness="0">清空</Button>
        </DockPanel>
    </Grid>
</UserControl>
